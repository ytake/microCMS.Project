{
  "name": "microCms.project",
  "version": "0.0.1",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "react": "0.*",
    "highlightjs" : "*",
    "bootstrap": "*"
  },
  "exportsOverride": {
  },
  "overrides": {
    "react": {
      "main": [
        "**/*.min.js"
      ]
    },
    "bootstrap": {
      "main": [
        "**/*.min.js",
        "**/*.min.css",
        "**/glyphicons-*"
      ]
    },
    "highlightjs": {
      "main": [
        "**/*.pack.js",
        "**/github.css"
      ]
    }
  }
}
