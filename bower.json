{
    "name": "microcms-project",
    "version": "0.0.1",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "dependencies": {
        "bootstrap": "*",
        "react": "0.*",
        "fancybox": "2.*",
        "font-awesome": "*"
    },
    "exportsOverride": {

    },
    "overrides": {
        "font-awesome": {
            "main" : [
                "**/*.min.css",
                "**/Font*",
                "**/*webfont*"
            ]
        },
        "bootstrap": {
            "main" : [
                "**/*.min.css",
                "**/glyphicons-*"
            ]
        },
        "jquery": {
            "main" : "**/*.min.js"
        },
        "fancybox": {
            "main": [
                "**/*.pack.js",
                "**/*.css",
                "**/*.png",
                "**/*.gif"
            ]
        },
        "react": {
            "main" : [
                "**/*.min.js",
                "JSXTransformer.js"
            ]
        }
    }
}
